xterm*internalBorder: 14
xterm*termName: xterm-256color
xterm*locale: false
xterm*utf8 : true
xterm*metaSendsEscape: true

xterm*faceName: RobotoMono Nerd Font Mono:size=12
!xterm*faceName: JetBrains Mono:size=12

xterm*decTerminalID: vt340
xterm*underlineURLs: true
xterm*urlLauncher: xdg-open
xterm*url-select.button: 1
xterm*urgentOnBell: true
xterm*geometry: 100x22
xterm*scrollstyle: plain
xterm*pointerBlank: true
xterm*scrollBar: false

! special
*.foreground:   #F2F1F0
*.background:   #202020
*.cursorColor:  #5B8A9A

! black
*.color0:       #222222
*.color8:       #666666

! red
*.color1:       #E84F4F
*.color9:       #D23D3D

! green
*.color2:       #B7CE42
*.color10:      #BDE077

! yellow
*.color3:       #C4E969
*.color11:      #C4E969

! blue
*.color4:       #66AABB
*.color12:      #AACCBB

! magenta
*.color5:       #B7416E
*.color13:      #E16A98

! cyan
*.color6:       #6D878D
*.color14:      #42717B

! white
*.color7:       #F2F1F0
*.color15:      #F2F1F0

